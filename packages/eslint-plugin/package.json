{
  "name": "@physicaldotart/eslint-plugin",
  "version": "1.0.0",
  "description": "Extracts all TailwindCss class names located in an HTML Tag, outsources the class names to a constant, and updates the HTML tag with the constant",
  "keywords": [
    "tailwindcss-eslint-plugin",
    "eslintplugin",
    "eslint-plugin",
    "eslint",
    "tailwindcss"
  ],
  "author": "BennoDev",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "shx rm -rf dist && tsc",
    "install:clean": "shx rm -rf node_modules && pnpm install",
    "test": "jest",
    "start": "ts-node ./src/index.ts"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.50.0",
    "escalade": "^3.1.1",
    "import-fresh": "^3.3.0",
    "lodash.get": "^4.4.2",
    "object-hash": "^3.0.0",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.7",
    "@types/object-hash": "^3.0.2",
    "@types/requireindex": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "jest": "^29.4.1",
    "shx": "^0.3.4",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
